{
  "name": "vue-detour",
  "description": "Introduce users to your app by taking them on a detour.",
  "version": "0.0.9",
  "license": "MIT",
  "author": {
    "name": "Hayden Woods",
    "email": "haydenwoodsdeveloper@gmail.com",
    "url": "https://hayden.sh"
  },
  "homepage": "https://github.com/haydenwoods/vue-detour",
  "bugs": "https://github.com/haydenwoods/vue-detour/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/haydenwoods/vue-detour.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "javascript",
    "js",
    "typescript",
    "ts"
  ],
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "main": "./dist/vue-detour.umd.js",
  "module": "./dist/vue-detour.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/vue-detour.es.js",
      "require": "./dist/vue-detour.umd.js"
    }
  },
  "scripts": {
    "dev": "vite --config ./vite.dev.config.ts",
    "build": "vue-tsc --noEmit && vite build --config ./vite.lib.config.ts",
    "publish": "npm publish",
    "publish:alpha": "npm publish --tag alpha",
    "publish:beta": "npm publish --tag beta"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "vite-plugin-dts": "^1.1.1",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/node": "^17.0.25",
    "@vitejs/plugin-vue": "^2.3.1",
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.5.4",
    "vite": "^2.9.5",
    "vue-tsc": "^0.34.7"
  }
}
